{
  "name": "cv-index-search-api",
  "version": "0.0.1",
  "description": "A CV indexing and searching api",
  "main": "index.js",
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "start": "nodemon",
    "transpile": "tsc",
    "clean": "rimraf build",
    "sync-env": "sync-dotenv",
    "build": "run-s clean transpile",
    "test": "NODE_ENV=test yarn migrate && NODE_ENV=test jest --forceExit --detectOpenHandles --maxWorkers=1 --verbose",
    "lint": "eslint . --ext .ts,.json",
    "lint:fix": "eslint --fix . --ext .ts,.json",
    "prettify": "prettier 'src/**/*.ts' --write"
  },
  "private": true,
  "license": "MIT",
  "keywords": [
    "api",
    "es6",
    "node",
    "express",
    "javascript",
    "typescript"
  ],
  "dependencies": {
    "@elastic/elasticsearch": "^7.15.0",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.11.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "morgan": "^1.10.0",
    "pdf2json": "^1.2.5",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.4",
    "sync-dotenv": "^1.0.0",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
